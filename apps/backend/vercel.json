{
    "version": 2,
    "builds": [
        {
            "src": "apps/backend/package.json",
            "use": "@vercel/node",
            "config": {
                "outputDirectory": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "apps/backend/dist/server.js"
        }
    ]
}